package com.rules4j.engine;

import java.util.Map;
import java.util.Set;

import com.rules4j.data.IRule;

public interface IRuleEngine {
    Set<IRule> Evaluate(Map<String, Object> data);
    Set<IRule> Evaluate(Object data);
    Set<IRule> EvaluateAndFire(Map<String, Object> data);
    Set<IRule> EvaluateAndFire(Object data);
}
